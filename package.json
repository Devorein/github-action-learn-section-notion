  "dependencies": {
    "@actions/core": "^1.2.7",
    "@nishans/endpoints": "^0.0.32"
  },
  "devDependencies": {
    "@nishans/types": "^0.0.35",
    "@types/node": "^15.0.1",
    "@zeit/ncc": "^0.22.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  }
